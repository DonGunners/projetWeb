RewriteRule ^competition/([0-9]+)/delete/confirmation$ /controller/pageConfirmationSuppressionCompetition.controller.php?idC=$1 [L]
RewriteRule ^competition/([0-9]+)/update$ /controller/modifierCompetition.controller.php?idC=$1 [L]
RewriteRule ^competition/([0-9]+)/update/confirmation$ /view/pageConfirmationModificationCompetition.php [L]
RewriteRule ^competition/([0-9]+)/phase$ /controller/adminPhase.controller.php?idC=$1 [L]
