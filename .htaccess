RewriteRule ^competition/([0-9]+)/phase/([0-9]+)/delete$ /controller/supprimerPhase.controller.php?idC=$1&idP=$2 [L]
RewriteRule ^competition/([0-9]+)/phase/([0-9]+)/delete/confirmation$ /controller/pageConfirmationSuppressionPhase.controller.php?idC=$1&idP=$2 [L]
RewriteRule ^competition/([0-9]+)/phase/([0-9]+)/update$ /controller/modifierPhase.controller.php?idC=$1&idP=$2 [L]
RewriteRule ^competition/([0-9]+)/phase/([0-9]+)/update/confirmation$ /view/pageConfirmationModificationPhase.php?idC=$1 [L]
RewriteRule ^competition/([0-9]+)/phase/([0-9]+)/match$ /controller/adminMatch.controller.php?idC=$1&idP=$2 [L]
RewriteRule ^competition/([0-9]+)/phase/([0-9]+)/match/add/confirmation$ /controller/PageConfirmationAjoutMatch.controller.php?idC=$1&idP=$2 [L]
RewriteRule ^competition/([0-9]+)/phase/([0-9]+)/match/([0-9]+)/delete$ /controller/supprimerMatch.controller.php?idC=$1&idP=$2&idM=$3 [L]
RewriteRule ^competition/([0-9]+)/phase/([0-9]+)/match/([0-9]+)/delete/confirmation$ /controller/pageConfirmationSuppressionMatch.controller.php?idC=$1&idP=$2&idM=$3 [L]
RewriteRule ^competition/([0-9]+)/phase/([0-9]+)/match/([0-9]+)/update$ /controller/modifierMatch.controller.php?idC=$1&idP=$2&idM=$3 [L]
RewriteRule ^competition/([0-9]+)/phase/([0-9]+)/match/([0-9]+)/update/confirmation$ /view/pageConfirmationModificationMatch.php?idC=$1&idP=$2 [L]
RewriteRule ^admins/add$ /controller/ajouterAdmin.controller.php [L]
RewriteRule ^admins/update/mdp$ /controller/modifierMdpAdmin.controller.php [L]
RewriteRule ^admins/update/mdp/confirmation$ /view/pageConfirmationModificationMdpAdmin.php [L]
RewriteRule ^admins/update/email$ /controller/modifierEmailAdmin.controller.php [L]
RewriteRule ^admins/update/email/confirmation$ /view/pageConfirmationModificationEmailAdmin.php [L]
RewriteRule ^admins/([0-9]+)/delete$ /controller/supprimerAdmin.controller.php?idA=$1 [L]
RewriteRule ^admins/([0-9]+)/delete/confirmation$ /controller/pageConfirmationSuppressionAdmin.controller.php?idA=$1 [L]